{
	"manifest_version": 2,

	"name": "Declassicle",
	"description": "This extension brings games and puzzles to your reading.",
	"version": "1.0",

	"background": {
		"scripts": ["eventPage.js"]
	},

	"browser_action": {
	 "default_icon": "icon.png",
	 "default_popup": "popup.html",
	 "default_title": "I am a tool tip"
	},

	"options_page": "options.html",

	"permissions": [
	 "activeTab",
	 "tabs"
	 ]
}